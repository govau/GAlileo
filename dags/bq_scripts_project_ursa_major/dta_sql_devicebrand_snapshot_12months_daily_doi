create or replace table dta_project_ursa_major.device_brand_yearly_snapshot_month_delta_doi
    OPTIONS (
        description = "Monthly delta snapshot of past 12 months",
        expiration_timestamp = TIMESTAMP_ADD(current_timestamp, INTERVAL 1 HOUR)
    )
    as
      select 
        t.device_brand,
        t.month_year,
        t.device_brand_count,
        round((t.device_brand_count / (sum(t.device_brand_count) over (partition by t.month_year order by t.month_year)) * 100),2) as percent_month,
        t.post_stamp
      from  t_12months_snapshot_device_brand_monthlydelta_doi t
;

END;