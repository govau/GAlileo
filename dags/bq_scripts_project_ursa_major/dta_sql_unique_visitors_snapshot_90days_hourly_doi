    )
    GROUP BY visit_hour,
                      -- visit_date,
                      visit_weekday

    ;

    create or replace table dta_project_ursa_major.unique_visitors_90days_hourly_delta_doi
    OPTIONS (
        description = "Daily delta hourly snapshot of past 90 day for Project URSA Major",
        expiration_timestamp = TIMESTAMP_ADD(current_timestamp, INTERVAL 1 HOUR)
    )
    as
    select   
            case 
                when i.unique_visitors > i.unique_visitors_approx then i.unique_visitors
                else i.unique_visitors_approx
            end as total_unique_users,
            case 
                when i.unique_visitors > i.unique_visitors_approx then round(i.unique_visitors/100000,1)
                else round(i.unique_visitors_approx/100000, 1)
            end as total_unique_users_scale,
            -- i.visit_date,
            i.visit_hour,
            i.visit_weekday,
            i.posted_timestamp as posted_stamp
    from  t_unique_visitors_90days_hourly i
    order by 
              i.visit_weekday,
              i.visit_hour     
    ;


END;