    )
    GROUP BY   
                        device_browser,
                        visit_year,
                        visit_month,
                        month_year
   ;

   create or replace table dta_project_ursa_major.device_browser_yearly_snapshot_month_delta_doi
    OPTIONS (
        description = "Monthly delta snapshot of past 12 months",
        expiration_timestamp = TIMESTAMP_ADD(current_timestamp, INTERVAL 1 HOUR)
    )
    as
    select 
        t.device_browser,
        t.month_year,
        t.browser_count,
        round((t.browser_count / (sum(t.browser_count) over (partition by t.month_year order by t.month_year)) * 100),2) as percent_month,
        t.post_stamp
from  t_12months_snapshot_device_browser_montlydelta_doi  t
order by 
          t.visit_year desc,
          t.visit_month desc
;
    
END;