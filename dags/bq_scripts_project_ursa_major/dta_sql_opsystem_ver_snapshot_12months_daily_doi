                        device_opsys_ver,
                        visit_year,
                        visit_month,
                        month_year
    ) as t
   ;


   create or replace table dta_project_ursa_major.device_opsys_ver_yearly_snapshot_month_delta_doi
    OPTIONS (
        description = "Monthly delta snapshot of past 12 months",
        expiration_timestamp = TIMESTAMP_ADD(current_timestamp, INTERVAL 1 HOUR)
    )
    as
    select
      device_opsys_ver,
      month_year,
      opsys_version_count,
      percent_month,
      current_timestamp as post_stamp
      from (
            select 
                  device_opsys_ver,
                  month_year,
                  opsys_version_count,
                  percent_month,
                  visit_year, visit_month
            from  t_12months_snapshot_opsys_ver_monthlydelta_doi  
            where top_browser < 6
            union all
            select  
                  "Others" as device_opsys_ver,
                  month_year,
                  sum(opsys_version_count) as opsys_version_count,
                  round(sum(percent_month),2) as percent_month,
                  visit_year, visit_month
            from  t_12months_snapshot_opsys_ver_monthlydelta_doi
            where top_browser > 5
            group by month_year, visit_year, visit_month
    )
    order by 
          visit_year,
          visit_month,
          opsys_version_count desc
;
    
END;