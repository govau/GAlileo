   create or replace table dta_customers.device_opsbrowser_daily_snapshot_delta_doi
    OPTIONS (
        description = "Daily delta snapshot of previous day",
        expiration_timestamp = TIMESTAMP_ADD(current_timestamp, INTERVAL 1 HOUR)
    )
    as
    select 
    COALESCE(da.agency,"") as agency,
    t.reg_domain,
    t.device_ops,
    t.device_browser,
    t.ops_browser_count,
    t.record_date,
    CURRENT_TIMESTAMP() as posted_timestamp,
    t.full_day_date
from        t_pgvw_snapshot_device_opsbrowser_delta_doi  t
left join   dta_customers.domain_agency_map da
on           t.reg_domain = da.domain_name
;
    
END;