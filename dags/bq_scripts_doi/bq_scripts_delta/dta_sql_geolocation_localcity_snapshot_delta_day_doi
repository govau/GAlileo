create or replace table dta_customers.localcity_daily_snapshot_delta_doi
    OPTIONS (
        description = "Daily delta snapshot of past day",
        expiration_timestamp = TIMESTAMP_ADD(current_timestamp, INTERVAL 1 HOUR)
    )
    as
          select 
              COALESCE(da.agency,"") as agency,
              t.reg_domain,
              t.local_city,
              t.geo_y,
              t.geo_x,
              t.local_city_count,
              t.record_date,
              CURRENT_TIMESTAMP() as posted_timestamp,
              t.full_day_date
from        t_dta_delta_snapshot_local_city_doi  t
left join   dta_customers.domain_agency_map da
on           t.reg_domain = da.domain_name
;


END;